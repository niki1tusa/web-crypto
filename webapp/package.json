{
  "name": "@app/webapp",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache --cache-location node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache \"src/styles/**/*.scss\"",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "pretify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@app/backend": "file:../backend",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/react-query": "^5.72.2",
    "@trpc/client": "^11.0.4",
    "@trpc/react-query": "^11.0.4",
    "@trpc/tanstack-react-query": "^11.0.4",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0",
    "reset-css": "^5.0.2",
    "sass": "^1.86.3",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
