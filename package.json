        "pretify": "pnpm -r --parallel pretify"

    },
    "devDependencies": {
        "@eslint/compat": "^1.2.8",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.24.0",
        "concurrently": "^9.1.2",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-config-react-app": "^7.0.1",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.8.3"
    }
}
